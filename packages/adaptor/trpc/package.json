{
  "name": "@fire-shield/trpc",
  "version": "2.2.0",
  "description": "tRPC adapter for Fire Shield RBAC with type-safe middleware",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "rbac",
    "trpc",
    "access-control",
    "permissions",
    "fire-shield",
    "middleware",
    "type-safe"
  ],
  "author": "KentPhung92",
  "license": "DIB",
  "dependencies": {
    "@fire-shield/core": "2.2.0"
  },
  "peerDependencies": {
    "@trpc/server": "^10.0.0"
  },
  "devDependencies": {
    "@trpc/server": "^10.45.0",
    "@types/node": "^22.5.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^1.6.1",
    "zod": "^3.22.4"
  }
}
