{
  "name": "@fire-shield/react-native",
  "version": "2.2.3",
  "description": "React Native adapter for Fire Shield RBAC with hooks and components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "react-native": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "rbac",
    "react-native",
    "access-control",
    "permissions",
    "fire-shield",
    "hooks",
    "mobile"
  ],
  "author": "khapu2906",
  "license": "DIB",
  "dependencies": {
    "@fire-shield/core": "^2.2.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.5.4",
    "@types/node": "^22.5.1",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-native": "^0.73.0",
    "react-test-renderer": "^18.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^1.6.1"
  }
}
